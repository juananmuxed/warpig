{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "prettier.enable": false,
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript",
  ],
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.experimental.useFlatConfig": true,
  "html.format.enable": false,
  "emmet.triggerExpansionOnTab": true,
  "files.exclude": {
    "**/*.log": true,
    "**/*.log*": true,
    "**/dist": false
  },
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".markdownlintrc": "jsonc",
    "*.config.js": "javascript",
    "*.spec.js": "javascript",
  },
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "onWindowChange",
  "turboConsoleLog.quote": "'",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "todo-tree.highlights.defaultHighlight": {
    "type": "text",
    "opacity": 80
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check",
      "iconColour": "#39c5cf",
      "background": "#00575e79",
      "gutterIcon": true
    },
    "FIXME": {
      "foreground": "#f8f8f8",
      "background": "#b82525",
      "iconColour": "#dd7d00",
      "gutterIcon": true
    }
  },
  "cSpell.language": "en,es",
  "i18n-ally.displayLanguage": "es",
  "i18n-ally.extract.keyMaxLength": 145,
  "i18n-ally.sourceLanguage": "es",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "locales",
  "i18n-ally.sortKeys": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "testing.automaticallyOpenPeekView": "never",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#19858c",
    "activityBar.background": "#19858c",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ec93e6",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#19858c",
    "statusBar.background": "#115c61",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#19858c",
    "statusBarItem.remoteBackground": "#115c61",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#115c61",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#115c6199",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "cSpell.words": [
    "signup",
    "TINYINT"
  ]
}
